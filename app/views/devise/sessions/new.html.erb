<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
<div class='form-wrap'>
  <div class='form-header'>
    <h1 class='form-header-text'>
      ログイン
    </h1>
  </div>
  <div class="form-group">
    <div class='form-text-wrap'>
      <label class="form-text">メールアドレス</label>
    </div>
    <%= f.email_field :email, class:"input-default", id:"email", placeholder:"登録したEmail", autofocus: true %>
  </div>
  <div class="form-group">
    <div class='form-text-wrap'>
      <label class="form-text">パスワード</label>
    </div>
    <%= f.password_field :password, class:"input-default", id:"password", placeholder:"パスワード" %>
  </div>
  <div class="actions">
    <%= f.submit "Log in" %>
  </div>
</div>
<% end %>
<%= link_to "アカウントをお持ちでない方はこちらで作成！", new_user_registration_path%>


